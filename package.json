{
  "name": "UnitTesting",
  "description": "React Component Unit Testing",
  "author": "Patrick van Vuuren <patr_vv@hotmail.com>",
  "version": "1.0.0",
  "engines-strict": true,
  "engines": {
    "node": "^6.0.0"
  },
  "repository": "https://github.com/Serfenia/ReactUnitTesting.git",
  "scripts": {
    "test": "jest --coverage"
  },
  "dependencies": {
    "jest-cli": "^14.0.1",
    "react": "^15.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-jest": "^14.1.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js"],
    "moduleFileExtensions": ["js", "json", "es6"],
    "unmockedModulePathPatterns": ["<rootDir>/node_modules/react"]
  }
}
